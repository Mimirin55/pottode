<!DOCTYPE html>
<html>
  <head>
    <title>Pottode</title>
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag "application" %>
  </head>

  <body>
    <%= render 'tasks/header' %>
    <%= yield %>
    <%= render 'tasks/footer' %>
  </body>
</html>
